{
    "$schema": "https://raw.githubusercontent.com/Polymer/tachometer/master/config.schema.json",
    "sampleSize": 10,
    "timeout": 0,
    "benchmarks": [
        {
            "name": "fast-element",
            "url": "fast-element/index.html",
            "browser": {
                "name": "chrome",
                "headless": false
            },
            "measurement": [
                {
                    "name": "duration",
                    "mode": "performance",
                    "entryName": "render"
                }
            ],

            "expand": [
                {
                    "name": "current-version",
                    "packageVersions": {
                        "label": "1.8.0",
                        "dependencies": {
                            "@microsoft/fast-element": "1.8.0"
                        }
                    }
                },
                {
                    "name": "previous-version",
                    "packageVersions": {
                        "label": "1.4.0",
                        "dependencies": {
                            "@microsoft/fast-element": "1.4.0"
                        }
                    }
                },
                {
                    "name": "local-version",
                    "packageVersions": {
                        "label": "local",
                        "dependencies": {
                            "@microsoft/fast-element": {
                                "kind": "git",
                                "repo": "https://github.com/microsoft/fast.git",
                                "ref": "master",
                                "subdir": "packages/web-components/fast-element",
                                "setupCommands": ["npm run build"]
                            }
                        }
                    }
                }
            ]
        }
    ]
}
